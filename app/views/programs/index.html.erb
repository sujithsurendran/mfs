<h1>Listing programs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Qualification</th>
    <th>Syllabus detailed</th>
    <th>Duration</th>
    <th>Fees</th>
    <th>Timing</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @programs.each do |program| %>
  <tr>
    <td><%= program.name %></td>
    <td><%= program.qualification %></td>
    <td><%= program.syllabus_detailed %></td>
    <td><%= program.duration %></td>
    <td><%= program.fees %></td>
    <td><%= program.timing %></td>
    <td><%= link_to 'Show', program %></td>
    <td><%= link_to 'Edit', edit_program_path(program) %></td>
    <td><%= link_to 'Destroy', program, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Program', new_program_path %>
